//Faça um algoritmo que leia dois valores e escreva os dois em ordem crescente
#include<stdio.h>
int main ()
{
    int num1, num2;
    int maior, menor ;
    
    printf("Insira um valor: ");
    scanf ("%d", &num1);
    
    printf("Insira um valor: ");
    scanf ("%d", &num2);
    
    if (num1>num2)
    {
       maior = num1;
       menor = num2;
    }
    else 
    if (num2 > num1)
    {
      maior = num2;
      menor = num1;
    }    
    else 
    {
        printf ("Números iguais.");
        return 0;
    }
    
    printf("Ordem crescente: %d %d", maior, menor);
    
    return 0;
}
